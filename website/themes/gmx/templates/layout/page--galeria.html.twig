<div id="page-wrapper">
  <div id="page">


  <!--Start: Branding and Menu -->
<div id="stick">
  <div class="menu">
    <div class="container">
      <div class="row">
        {% if page.header %}
          <div class="col-xs-6 col-md-4">
            {{ page.header }}
          </div>
        {% endif %}
        <!-- Navigation -->
        {% if page.menu %}
          <div class="col-xs-6 col-md-8">
            {{ page.menu }} 
          </div>
        {% endif %}
        <!--End Navigation -->
      </div>
    </div>
  </div>
</div>

<!-- Header and Navbar -->
<header class="main-galeria">

  <!-- Start: Banner -->
    {% if page.welcome_banner_text %}
      <div class="container">
        <div class="banner">
          <div class="row">
            <div class="col-md-12">
              {{ page.welcome_banner_text }}
            </div>
          </div>
        </div>
      </div>
    {% endif %}
  <!-- End: Banner -->


    {% if page.messages %}
        <div class="container">
          <div class="row">
            <div class="col-md-12">{{ page.messages }}</div>
          </div>
        </div>
    {% endif %}


</header>
<!--End Header & Navbar -->


      <!--- Start content -->
      {% if page.content %}
        <div class="container-fluid">
          <div class="row contenidogaleria">
              {{ page.content }}
          </div>
        </div>
      {% endif %}
      <!---End content -->


<!--Start: Footer -->
  <div class="footer">
    <div class="container">
      <div class="row">
        <div class="col-md-12">
          {{ page.footera }}
        </div>
      </div>
    </div>
  </div>
<!--End: Footer -->

  </div>
</div>
